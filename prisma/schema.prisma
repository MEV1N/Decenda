generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Registration {
  id             String   @id @default(cuid())
  teamName       String
  name           String
  email          String   @unique
  phone          String   @unique
  college        String
  member2Name    String?
  member2Email   String?  @unique
  member2Phone   String?  @unique
  member2College String?
  createdAt      DateTime @default(now())
}
